{
  "name": "tomskjs.ru",
  "version": "0.0.0",
  "description": "tomskjs site",
  "scripts": {
    "lint": "npm run lint:css --silent && npm run lint:js --silent",
    "lint:js": "eslint --ext .jsx --ext .js app server utils",
    "lint:css": "stylelint 'app/**/*.css'",
    "fix": "npm run lint:js -- --fix",
    "pretest": "npm run lint --silent",
    "test": "karma start --single-run=true",
    "build": "better-npm-run build",
    "start": "better-npm-run start",
    "playground": "better-npm-run playground",
    "production": "better-npm-run production",
    "commit": "commit-wizard"
  },
  "betterScripts": {
    "build": {
      "command": "babel-node utils/build.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "nodemon --watch server/* --exec babel-node server/server",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "playground": {
      "command": "babel-node server/server",
      "env": {
        "NODE_ENV": "playground"
      }
    },
    "production": {
      "command": "babel-node server/server",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.2.3",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-source-wrapper": "^1.3.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-typecheck": "^3.5.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-1": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "chai-things": "^0.2.0",
    "classnames": "^2.2.2",
    "cosmos-js": "Nitive/cosmos",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "deep-freeze-node": "^1.1.1",
    "enzyme": "^1.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-plugin-react": "^3.13.1",
    "express": "^4.13.3",
    "express-debuga": "^1.0.0",
    "express-open-in-editor": "^1.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
