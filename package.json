  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomskjs/tomskjs.ru.git"
  },
  "keywords": [
    "tomsk",
    "js",
    "tomskjs"
  ],
  "author": "Maxim Samoilov @nitive",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomskjs/tomskjs.ru/issues"
  },
  "homepage": "https://github.com/tomskjs/tomskjs.ru#readme",
  "dependencies": {
    "better-npm-run": "0.0.5",
    "colors": "^1.1.2",
    "jade": "^1.11.0",
    "node-bin": "^5.3.0"
  },
  "config": {
    "pre-git": {
      "commit-msg": "conventional",
      "pre-commit": [],
      "pre-push": [
        "npm test"
      ],
      "post-commit": [],
      "post-merge": []
    }
  }
}
