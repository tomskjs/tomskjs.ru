    "gemini": "^2.0.3",
    "gemini-gui": "^2.0.0",
    "history": "^1.17.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.16",
    "karma-chai": "^0.1.0",
    "karma-chai-things": "^0.1.4",
    "karma-chrome-launcher": "^0.2.2",
    "karma-mocha": "^0.2.1",
